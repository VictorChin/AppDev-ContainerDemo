{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "databaseAccounts_documentdb_name": {
            "defaultValue": null,
            "type": "SecureString"
        },
        "databaseAccounts_documentdb_name_1": {
            "defaultValue": "VALUEOF-UNIQUE-SERVER-PREFIX-documentdb",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [       
        {
            "comments": "Generalized from resource",
            "type": "Microsoft.DocumentDB/databaseAccounts",
            "kind": "MongoDB",
            "name": "[parameters('databaseAccounts_documentdb_name_1')]",
            "apiVersion": "2015-04-08",
            "location": "East US 2",
            "tags": {},
            "properties": {
                "databaseAccountOfferType": "Standard",
                "consistencyPolicy": {
                    "defaultConsistencyLevel": "Session",
                    "maxIntervalInSeconds": 5,
                    "maxStalenessPrefix": 100
                },
                "name": "[parameters('databaseAccounts_documentdb_name')]"
            },
            "dependsOn": []
        },
        
        
    ]
}